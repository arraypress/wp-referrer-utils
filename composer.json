{
  "name": "arraypress/wp-referrer-utils",
  "version": "1.0.0",
  "description": "A lean WordPress library for HTTP referrer detection and analysis",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-referrer-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\ReferrerUtils\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-referrer-utils/issues",
    "source": "https://github.com/arraypress/wp-referrer-utils"
  },
  "keywords": [
    "wordpress",
    "referrer",
    "referer",
    "http-referrer",
    "analytics",
    "traffic-source",
    "search-engine",
    "social-media",
    "utm",
    "arraypress"
  ]
}